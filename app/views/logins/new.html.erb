<h1>Log in</h1>

<%= form_with model: @user, url: logins_path do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Log in" %>
  </div>
<% end %>

<div style="margin-top:2rem"> 
<%= link_to "Sign Up", new_registration_path %>
<%= link_to "View Posts", posts_path %>
</div>
